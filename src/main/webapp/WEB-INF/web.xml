<web-app xmlns="http://jakarta.ee/xml/ns/jakartaee" version="5.0">
  <servlet>
    <servlet-name>liste</servlet-name>
    <servlet-class>com.example.servlet.ListeEtudiantsServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>liste</servlet-name>
    <url-pattern>/etudiants</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>NoteServlet</servlet-name>
    <servlet-class>com.example.servlet.NoteServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>NoteServlet</servlet-name>
    <url-pattern>/notes</url-pattern>

  <servlet>
    <servlet-name>listeClasses</servlet-name>
    <servlet-class>com.example.servlet.ListeClasseServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>listeClasses</servlet-name>
    <url-pattern>/classes</url-pattern>
  </servlet-mapping>

  <!-- Salle -->
  <servlet>
    <servlet-name>SalleListe</servlet-name>
    <servlet-class>com.example.servlet.ListeSallesServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>SalleListe</servlet-name>
    <url-pattern>/salles</url-pattern>
  </servlet-mapping>
</web-app>
